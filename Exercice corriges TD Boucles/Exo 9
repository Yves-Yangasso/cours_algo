Algorithme Exo9
Variable N,I,J,cpt : entier
Debut
	Répeter 
		Ecrire("Saisir une valeur entiere positive:")
		Lire(N)
	Jusqu'à (N > 0)
	Pour I <-- 1 à N Faire
		cpt <-- 0
		Pour J <-- 1 à I Faire
			Si ((I mod J) = 0) alors
				cpt <-- cpt + 1
			Finsi
		FinPour	
		Si (cpt = 2) alors
			Ecrire(I,"est un nombre premier")
		Finsi
	FinPour
Fin
			